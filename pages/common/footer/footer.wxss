@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype'),
        url('iconfont.svg#iconfont') format('svg');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:20px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.footer {
  height: 98rpx;
  width: 100%;
  position: fixed;
  bottom: 0rpx;
  text-align: center;
  display: flex;
  background-color: #fff;
  font-size: 24rpx;
  align-items: center;
}
.footer_item {
  flex: 1;
  color: #333;
}
.footer_active {
  color: #1AAD19;
  fill: #1AAD19;
}

.home:after {
  content: "\e614";
}
.explore:after {
  content: "\e659";
}
.tool:after {
  content: "\e709";
}
.me:after {
  content: "\e60e";
}